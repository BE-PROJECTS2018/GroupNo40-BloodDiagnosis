<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="health.care.com.BEproj.Main3Activity"
    tools:showIn="@layout/activity_main3">


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="Diagnostic Tests"
        android:textAppearance="?android:attr/textAppearanceLarge"
        tools:layout_editor_absoluteX="109dp"
        tools:layout_editor_absoluteY="35dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_Malaria"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Test For Malaria"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="116dp"
        tools:layout_editor_absoluteY="116dp" />

    <Button
        android:id="@+id/button_Leukemia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Test for Leukemia"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="113dp"
        tools:layout_editor_absoluteY="212dp" />
</android.support.constraint.ConstraintLayout>
